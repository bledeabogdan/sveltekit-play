{
	"name": "sveltekit-play",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@askia/fibre-colors": "1.2.6",
		"@askia/fibre-components": "1.73.0",
		"@askia/fibre-grid": "1.2.5",
		"@askia/fibre-icons": "1.4.37",
		"@askia/fibre-layout": "1.2.5",
		"@askia/fibre-motion": "1.2.5",
		"@askia/fibre-styles": "1.55.3",
		"@askia/fibre-themes": "1.2.7",
		"@askia/fibre-type": "1.3.0",
		"@playwright/test": "^1.44.1",
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/kit": "^2.5.10",
		"@typescript-eslint/eslint-plugin": "^7.12.0",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@typescript-eslint/parser": "^7.12.0",
		"eslint": "^9.4.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"prettier": "^3.3.0",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.17",
		"svelte-check": "^3.8.0",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.12",
		"vitest": "^1.6.0"
	},
	"type": "module"
}
